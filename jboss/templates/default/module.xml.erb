<?xml version="1.0" encoding="UTF-8"?>
  <module xmlns="urn:jboss:module:1.0" name="<%= @mod['package'] %>">
   <resources>
            <resource-root path="<%= "#{@name}-#{@mod['version']}.jar" %>"/>
   </resources>
  <dependencies>
    <% @mod['dependencies'].each do |dep| -%>
    <module name="<%= dep %>"/>
    <% end -%>
  </dependencies>
</module>
